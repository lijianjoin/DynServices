    <!-- Creates the available routine master data table -->
    <changeSet author="ismart2" id="master-data-available-routines-v0.0.1">
        <!-- ProductTypeEntity -->
        <createTable remarks="All available routines for the system"
                     schemaName="available_routines"
                     tableName="available_routines">
            <column name="id" type="uuid">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false" primaryKey="false"/>
            </column>
            <column name="version" type="varchar(255)">
                <constraints nullable="false" primaryKey="false"/>
            </column>
            <column name="type" type="varchar(255)">
                <constraints nullable="false" primaryKey="false"/>
            </column>
            <column name="description" type="varchar(255)">
                <constraints nullable="true" primaryKey="false"/>
            </column>
        </createTable>

        <addUniqueConstraint
                schemaName="available_routines"
                tableName="available_routines"
                columnNames="name, version"
                constraintName="unique_masterdata_for_name_version"
        />

